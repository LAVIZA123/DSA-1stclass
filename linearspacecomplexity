// Online C++ compiler to run C++ program online
#include <iostream>
#include <vector> // Required for vector
using namespace std;

// Linear space complexity example:
vector<int> doubleArray(int arr[], int n) {
    vector<int> result(n);
    for (int i = 0; i < n; i++) {
        result[i] = 2 * arr[i];
    }
    return result;
}

int main() {
    int arr[] = {1, 2, 3, 4};
    vector<int> a = doubleArray(arr, 4);
    for (int x : a) {
        cout << x << " ";
    }
    return 0;
}
